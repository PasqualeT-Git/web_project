<div class="container-overall-about">
  <div class="banner-hero__about">
    <div class="d-flex justify-content-end">
      <img src="<%= image_path 'gray-scale-logo.svg' %>" alt="overall-logo gray-scale">
    </div>
    <div class="banner-hero__text">
      <h1>we are <span id="typing"></span><br> your projects</h1>
    </div>
  </div>
</div>
<div class="team-content">
  <div class="team-about__title">
    <%= image_tag 'logo.svg', class: "" %>
    <h3>us</h3>
  </div>
  <div class="team-content__group">
    <div class="team-single-content">
      <%= image_tag 'pasquale-overall.png' %>
      <div class="team-single__description">
        <h5>PASQUALE TROISE</h5>
        <p>Co founder |<br> full stack dev.</p>
      </div>
    </div>
    <div class="team-single-content" id="team-single-content__fabre">
      <div class="team-single__description">
        <h5>FABRE MARTINEZ</h5>
        <p>Co founder |<br> UI/UX & front dev.</p>
      </div>
      <%= image_tag 'fabre-overall.png' %>
    </div>
    <div class="team-single-content" id="team-single-content__morgan">
      <%= image_tag 'morgan-about_us.png' %>
      <div class="team-single__description">
        <h5>MORGAN LIZOP</h5>
        <p>Co Founder |<br> Project Manager</p>
      </div>
    </div>
  </div>
</div>

<div class="story-group">
  <div class="story-group__description">
    <p>We are a group of Le Wagon London alumni who love to turn other people's ideas into websites.</p><br>
    <p>We combine all of our knowledge to give to you the best result.</p>
    <%= image_tag 'coulored-logo.svg' %>
  </div>
</div>


<script>
  ScrollReveal().reveal('.container-overall-about',
                        { delay: 800 },
                        { duration: 2400 });
  ScrollReveal().reveal('.team-content__group',
                        { delay: 800 },
                        { duration: 800 });
</script>
